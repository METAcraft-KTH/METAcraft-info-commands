{
  "schemaVersion": 1,
  "id": "metacraftinfocommands",
  "version": "${version}",

  "name": "METAcraft info commands",
  "description": "A mod that allows the creation of custom information commands from a config file.",
  "authors": [
    "Acuadragon100"
  ],
  "contact": {
    "homepage": "https://github.com/METAcraft-KTH/METAcraft-info-commands",
    "sources": "https://github.com/METAcraft-KTH/METAcraft-info-commands"
  },

  "license": "",
  "icon": "assets/metacraftinfocommands/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "se.datasektionen.mc.infocommands.Info"
    ]
  },
  "mixins": [
    "metacraftinfocommands.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.20.x",
    "java": ">=17"
  }
}
